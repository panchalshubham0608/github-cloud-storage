{
  "name": "@panchalshubham0608/github-cloud-storage",
  "version": "1.2.0",
  "description": "Library to interact with GitHub repositories as a cloud-storage service!",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "index.js",
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "no-console": 1,
      "no-extra-boolean-cast": 0,
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "eslintIgnore": [
    "src/public/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panchalshubham0608/github-cloud-storage.git"
  },
  "keywords": [
    "github",
    "cloud-storage"
  ],
  "author": "@panchalshubham0608",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/panchalshubham0608/github-cloud-storage/issues"
  },
  "homepage": "https://github.com/panchalshubham0608/github-cloud-storage#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "http-status-codes": "^2.2.0"
  },
  "devDependencies": {
    "@jest/globals": "^28.1.3",
    "@types/jest": "^28.1.8",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "jest": "^28.1.3",
    "jest-config": "^28.1.3",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
